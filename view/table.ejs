<!DOCTYPE html>
<html lang="pt-br">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Table</title>

        <style>
            * {
                margin: 0;
                padding: 0;
            }
            .table {
                position: absolute;
                width: 100%;
                margin: auto;
                text-align: center;
            }

            .table h1 {
                color: red;
            }

            .table table {
                margin: auto;
            }
        </style>
    </head>
    <body>
        
        <div class="table">
            <h1>Table created with GUIACONVERTER</h1> <br>

            <table border="1">
                <thead>       
                    <% tableHeader.forEach(column => { %>
                        <th><%= column %></th>
                    <% }) %>
                </thead>
    
                <tbody>
                    <% tableBody.forEach(rows => { %>
                        <tr>
                            <% rows.forEach(row => { %>
                                <td><%= row %></td>
                            <% }) %>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>

    </body>
</html>